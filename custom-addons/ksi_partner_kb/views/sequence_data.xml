<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <data noupdate="0">
        <!-- Format: M/CompanyCode/DDMMSeq (Joining date)] -->

        <!-- DC : Klik Bangunan -->
        <record id="seq_res_partner_dc" model="ir.sequence">
            <field name="name">Customer Id - Klik Bangunan (DC)</field>
            <field name="code">res.partner</field>
            <field name="prefix">M/DC/%(day)s%(month)s</field>
            <field name="padding">5</field>
            <field name="company_id" eval="1"/>
            <!-- SET 1 Company ID! (select id from res_company where id=1)-->
        </record>

        <!-- KB01 : Klik Bangunan -->
